#!/usr/bin/make -f

export DH_VERBOSE=1

# explicitly use gcc 4.8
export CC=gcc-4.8
export CXX=g++-4.8
export CPPFLAGS=-I/usr/include/libxml2 -I/usr/include/x86_64-linux-gnu/ruby-2.1.0 -I/usr/include/python2.7

%:
	dh $@ --parallel

override_dh_auto_build:
	$(MAKE) -j4
       
# disable tests for faster builds
override_dh_auto_test:

